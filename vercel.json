{
  "builds": [
    {
      "src": "RentalCar/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb",
        "runtime": "python3.10.11",
        "installCommand": "pip install -r requirements.txt",
        "buildCommand": "python manage.py collectstatic --noinput && echo 'Collectstatic completed'"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "RentalCar/wsgi.py"
    }
  ]
}
